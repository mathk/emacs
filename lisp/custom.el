(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(backup-directory-alist (quote ((".*" . "~/.emacs.d/backups"))))
 '(erc-autojoin-channels-alist (quote (("freenode.net" "#gnu-smalltalk" "#emacsfr" "#emacs"))))
 '(erc-log-channels-directory "~/log")
 '(erc-modules (quote (autoaway autojoin button completion fill irccontrols list log match menu move-to-prompt netsplit networks noncommands readonly ring smiley stamp spelling track)))
 '(erc-nick "mathk")
 '(erc-nick-uniquifier "_")
 '(erc-pals (quote ("bonzini" "mrgwen" "NicolasPetton")))
 '(erc-port 7000)
 '(global-hi-lock-mode t)
 '(global-linum-mode t)
 '(icomplete-mode t)
 '(ido-mode (quote both) nil (ido))
 '(inhibit-startup-screen t)
 '(initial-frame-alist (quote ((cursor-type bar . 5))))
 '(isearch-allow-scroll t)
 '(linum-format "%d")
 '(scroll-bar-mode nil)
 '(semanticdb-default-save-directory "h:/.semanticdb")
 '(spell-command "aspell")
 '(tab-width 8)
 '(tool-bar-mode nil)
 '(user-full-name "Mathieu Suen")
 '(user-mail-address "mathieu.suen@zong.com"))
